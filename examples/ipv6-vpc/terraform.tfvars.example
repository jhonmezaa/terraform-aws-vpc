# Example terraform.tfvars for IPv6 VPC
# Copy this file to terraform.tfvars and customize the values
# This example demonstrates IPv6 dual-stack networking

# Note: Most variables have defaults in main.tf
# Override any values here as needed

# Example: Change region
# Update provider.tf to use a different region if needed

# Example: Customize VPC CIDR
# vpc_cidr_block = "10.3.0.0/16"

# Example: Change availability zones
# azs = ["a", "b", "c"]

# Example: Customize public subnets
# public_subnets = [
#   "10.3.1.0/24",
#   "10.3.2.0/24",
#   "10.3.3.0/24",
# ]

# Example: Customize IPv6 prefixes
# public_subnet_ipv6_prefixes = [0, 1, 2]
# private_subnet_ipv6_prefixes = [11, 12, 13]

# Example: Additional tags
# tags = {
#   Environment = "production"
#   IPv6        = "enabled"
#   Team        = "networking"
# }
